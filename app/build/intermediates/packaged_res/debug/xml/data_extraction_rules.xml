<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="clones"/>
        <include domain="file" path="virtual_registry"/>
        <include domain="file" path="virtual_environments"/>
        <exclude domain="file" path="temp"/>
    </cloud-backup>
    <device-transfer>
        <include domain="file" path="clones"/>
        <include domain="file" path="virtual_registry"/>
        <include domain="file" path="virtual_environments"/>
    </device-transfer>
</data-extraction-rules>
